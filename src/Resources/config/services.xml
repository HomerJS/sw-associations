<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="IhorAss\Core\Content\Simple\SimpleEntity">
            <tag name="shopware.entity" />
        </service>

        <service id="IhorAss\Core\Content\Simple\Aggregate\SimpleTranslation\SimpleTranslationEntity">
            <tag name="shopware.entity" />
        </service>

        <service id="IhorAss\Core\Content\Simple2\Simple2Entity">
            <tag name="shopware.entity" />
        </service>

        <service id="IhorAss\Core\Content\Simple2\Aggregate\Simple2Translation\Simple2TranslationEntity">
            <tag name="shopware.entity" />
        </service>

        <service id="IhorAss\Command\TestCommand">
            <tag name="console.command" />
            <argument type="service" id="simple_entity.repository" />
            <argument type="service" id="simple2_entity.repository" />
        </service>
    </services>
</container>